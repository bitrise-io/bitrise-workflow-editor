{
  "author": "Bitrise",
  "license": "MIT",
  "name": "bitrise-workflow-editor",
  "version": "2.5.14419",
  "description": "Bitrise workflow editor",
  "homepage": "https://github.com/bitrise-io/bitrise-workflow-editor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrise-io/bitrise-workflow-editor.git"
  },
  "bugs": {
    "url": "https://github.com/bitrise-io/bitrise-workflow-editor/issues"
  },
  "keywords": [
    "bitrise",
    "ci",
    "cd",
    "mobile",
    "pipeline",
    "workflow",
    "step",
    "editor"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint source/javascripts --cache",
    "lint:fix": "eslint source/javascripts --cache --quiet --fix",
    "build": "webpack --bail",
    "build:website": "MODE=WEBSITE ANALYTICS=true CLARITY=true DATADOG_RUM=true npm run build",
    "build:plugin": "MODE=CLI ANALYTICS=true npm run build && ./_scripts/compile_api.sh",
    "start:dev": "webpack serve",
    "start:dev:api": "DEV=true ./_scripts/run_api.sh 4000",
    "start:base": "concurrently \"npm:start:dev\" \"npm:start:dev:api\"",
    "start": "NODE_ENV=development MODE=CLI npm run start:base",
    "start:website": "NODE_ENV=development MODE=WEBSITE PUBLIC_URL_ROOT=http://localhost:4000 npm run start:base",
    "build-storybook": "storybook build -c .storybook",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:jest": "jest --reporters=\"default\" --reporters=\"jest-junit\"",
    "test:smoke": "playwright test",
    "generate-theme-types": "chakra-cli tokens node_modules/@bitrise/bitkit/src/theme.ts",
    "prepare": "husky",
    "knip": "knip"
  },
  "dependencies": {
    "@bitrise/bitkit": "13.280.0",
    "@dagrejs/dagre": "^1.1.4",
    "@datadog/browser-rum": "^6.8.0",
    "@datadog/browser-rum-react": "^6.8.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@monaco-editor/react": "^4.6.0",
    "@segment/analytics-next": "^1.81.0",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-virtual": "^3.13.9",
    "@xyflow/react": "^12.6.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "algoliasearch": "^5.25.0",
    "chakra-ui-2--react": "npm:@chakra-ui/react@2.10.9",
    "dequal": "^2.0.3",
    "es-toolkit": "^1.38.0",
    "launchdarkly-js-client-sdk": "^3.8.0",
    "monaco-editor": "0.51.0",
    "monaco-yaml": "^5.4.0",
    "node-diff3": "^3.1.2",
    "picomatch": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.4",
    "react-lazy-with-preload": "^2.2.1",
    "remove-markdown": "^0.6.0",
    "search-insights": "^2.17.3",
    "semver": "^7.7.2",
    "type-fest": "^4.41.0",
    "usehooks-ts": "^3.1.1",
    "wouter": "^3.7.1",
    "yaml": "^2.8.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@bitrise/eslint-plugin": "^4.0.4",
    "@google-cloud/storage": "^7.15.2",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-links": "^9.1.10",
    "@storybook/addon-queryparams": "^9.0.1",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
    "@storybook/react-webpack5": "^9.1.10",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.2",
    "@types/picomatch": "^4.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "chakra-ui-2--cli": "npm:@chakra-ui/cli@2.5.8",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^11.0.1",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "knip": "^5.50.5",
    "lint-staged": "^15.4.3",
    "mini-css-extract-plugin": "^2.9.4",
    "monaco-editor-webpack-plugin": "7.1.0",
    "msw": "^2.7.3",
    "msw-storybook-addon": "^2.0.6",
    "prettier": "^3.5.2",
    "storybook": "^9.1.10",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.8.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-subresource-integrity": "^5.1.0"
  },
