{
  "author": "Bitrise",
  "license": "MIT",
  "name": "bitrise-workflow-editor",
  "version": "2.5.14748",
  "description": "Bitrise workflow editor",
  "homepage": "https://github.com/bitrise-io/bitrise-workflow-editor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrise-io/bitrise-workflow-editor.git"
  },
  "bugs": {
    "url": "https://github.com/bitrise-io/bitrise-workflow-editor/issues"
  },
  "keywords": [
    "bitrise",
    "ci",
    "cd",
    "mobile",
    "pipeline",
    "workflow",
    "step",
    "editor"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint source/javascripts --cache",
    "lint:fix": "eslint source/javascripts --cache --quiet --fix",
    "build": "vite build",
    "build:website": "MODE=WEBSITE ANALYTICS=true CLARITY=true DATADOG_RUM=true npm run build",
    "build:plugin": "MODE=CLI ANALYTICS=true npm run build && ./_scripts/compile_api.sh",
    "start:dev": "vite",
    "start:dev:api": "DEV=true ./_scripts/run_api.sh 4000",
    "start:base": "concurrently \"npm:start:dev\" \"npm:start:dev:api\"",
    "start": "NODE_ENV=development MODE=CLI npm run start:base",
    "start:website": "NODE_ENV=development MODE=WEBSITE PUBLIC_URL_ROOT=http://localhost:4000 npm run start:base",
    "preview": "vite preview",
    "build-storybook": "storybook build -c .storybook",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:jest": "jest --reporters=\"default\" --reporters=\"jest-junit\"",
    "test:smoke": "playwright test",
    "generate-theme-types": "chakra-cli tokens node_modules/@bitrise/bitkit/src/theme.ts",
    "prepare": "husky",
    "knip": "knip"
  },
  "dependencies": {
    "@bitrise/bitkit": "^13.318.0",
    "@dagrejs/dagre": "^1.1.5",
    "@datadog/browser-rum": "^6.23.0",
    "@datadog/browser-rum-react": "^6.23.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@monaco-editor/react": "^4.6.0",
    "@segment/analytics-next": "^1.81.0",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-virtual": "^3.13.9",
    "@xyflow/react": "^12.9.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "algoliasearch": "^5.42.0",
    "bitrise-yml-lsp-server": "git+https://github.com/bitrise-io/bitrise-yml-language-server.git#929691744dc318c5b9a9834b039883a31cce6b07",
    "chakra-ui-2--react": "npm:@chakra-ui/react@2.10.9",
    "dequal": "^2.0.3",
    "es-toolkit": "^1.41.0",
    "launchdarkly-js-client-sdk": "^3.8.0",
    "monaco-editor": "^0.53.0",
    "monaco-yaml": "^5.4.0",
    "node-diff3": "^3.1.2",
    "picomatch": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-lazy-with-preload": "^2.2.1",
    "remove-markdown": "^0.6.0",
    "search-insights": "^2.17.3",
    "semver": "^7.7.3",
    "type-fest": "^5.1.0",
    "usehooks-ts": "^3.1.1",
    "wouter": "^3.7.1",
    "yaml": "^2.8.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@bitrise/eslint-plugin": "^4.0.4",
    "@google-cloud/storage": "^7.17.2",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-links": "^10.0.2",
    "@storybook/react-vite": "^10.0.2",
    "@swc/core": "^1.14.0",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.9.2",
    "@types/picomatch": "^4.0.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "chakra-ui-2--cli": "npm:@chakra-ui/cli@2.5.8",
    "concurrently": "^9.2.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-storybook": "^10.0.2",
    "glob": "^11.0.3",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "knip": "^5.66.4",
    "lint-staged": "^16.2.6",
    "msw": "^2.11.6",
    "msw-storybook-addon": "^2.0.6",
    "prettier": "^3.6.2",
    "storybook": "^10.0.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "wrangler": "^4.45.4"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/public"
    ]
  }
}
