	// Provisioning profiles

	requestService.getDetailsOfProvProfile = function(provProfileDatabaseID, requestConfig) {
		var requestURL;
		
		switch (requestService.mode) {
			case "website":
				requestURL = stringService.stringReplacedWithParameters("<%= webserver_path(data[:routes][:website][:prov_profile_get_details]) %>", {
					prov_profile_id: provProfileDatabaseID
