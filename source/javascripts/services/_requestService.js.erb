			case "cli":
				return $q.reject(new Error("<%= data[:strings][:request_service][:get_details_prov_profile][:not_website_mode_error] %>"));

				break;
		}

		return $q(function(resolve, reject) {
			$http.get(requestURL, requestConfig).then(function(response) {
				var developerCertificateIdentities = [];
				_.each(response.data.prov_profile_details.developer_certificates, function(aCertificateIdentity) {
					developerCertificateIdentities.push({
						commonName: aCertificateIdentity.common_name,
						startDate: aCertificateIdentity.start_date,
						endDate: aCertificateIdentity.end_date,
						teamName: aCertificateIdentity.team_name,
						teamID: aCertificateIdentity.team_id,
						serial: aCertificateIdentity.serial
					});
				});

				var capabilityKeys = [];
				_.each(response.data.prov_profile_details.entitlements, function(value, key) {
					capabilityKeys.push(key);
				});

				var provisionedDevices = [];
				_.each(response.data.prov_profile_details.provisioned_devices, function(value, key) {
					provisionedDevices.push({
						udid: value
					});
				});

				resolve({
					uuid: response.data.prov_profile_details.uuid,
					expirationDateAndTime: response.data.prov_profile_details.expiration_date,
					exportType: response.data.prov_profile_details.export_type,
					teamName: response.data.prov_profile_details.team_name,
					teamID: response.data.prov_profile_details.team_id,
					bundleID: response.data.prov_profile_details.bundle_id,
					developerCertificateIdentities: developerCertificateIdentities,
					capabilityKeys: capabilityKeys,
					provisionedDevices: provisionedDevices
				});
			}, function(response) {
				reject(errorFromResponse(response, "<%= data[:strings][:request_service][:get_details_prov_profile][:default_error] %>", "<%= data[:strings][:request_service][:get_details_prov_profile][:error_prefix] %>"));
			});
		});
	};

